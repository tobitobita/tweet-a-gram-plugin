<?xml version="1.0" encoding="UTF-8"?>
<plugin>
   <extension point="com.change_vision.astah.ui.actionSets">
      <actionSet
            label="%action_set_label"
            visible="true"
            id="tweet-a-gram-plugin.actionSet">
         <menu
               id="testplugin"
               label="%menu_group"
               path="tool/testplugin">
         </menu>
         <action
			   label="%menu_label"
               class="dsk.tweet_a_gram.plugin.twitter.action.TweetAction"
               menubarPath="tool/testplugin/"
               id="tweet-a-gram-plugin-twitter-tweet">
         </action>
         <action
			   label="%menu_label_logout"
               class="dsk.tweet_a_gram.plugin.twitter.action.LogoutAction"
               menubarPath="tool/testplugin/"
               id="tweet-a-gram-plugin-twitter-logout">
         </action>
     </actionSet>
   </extension>
</plugin>
